{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "target": "es6",
      "jsx": "react",
      "importHelpers": true,
      "lib": [
        "dom",
        "es2015"
      ]
  }
}